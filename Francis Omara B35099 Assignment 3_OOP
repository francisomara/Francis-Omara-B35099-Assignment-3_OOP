  {
   "cell_type": "code",
   "execution_count": 24,
   "id": "08c6890f-84d9-46b9-875b-9875ed5f3fa7",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Student ID: S022\n",
      "Name: Francis Omara\n",
      "Total Marks: 235\n",
      "Average: 78.33\n",
      "Grade: B\n"
     ]
    }
   ],
   "source": [
    "# 4.2. Methods: - Compute total and average marks. - Return grade (A â‰¥80, B â‰¥70, etc.).\n",
    "\n",
    "class Student:\n",
    "    def __init__(self, student_id, name, marks):\n",
    "        self.student_id = student_id\n",
    "        self.name = name\n",
    "        self.marks = marks\n",
    "\n",
    "    def compute_total(self):\n",
    "        return sum(self.marks.values())\n",
    "\n",
    "    def compute_average(self):\n",
    "        return self.compute_total() / len(self.marks)\n",
    "\n",
    "    def get_grade(self):\n",
    "        avg = self.compute_average()\n",
    "        if avg >= 80:\n",
    "            return \"A\"\n",
    "        elif avg >= 70:\n",
    "            return \"B\"\n",
    "        elif avg >= 60:\n",
    "            return \"C\"\n",
    "        elif avg >= 50:\n",
    "            return \"D\"\n",
    "        else:\n",
    "            return \"F\"\n",
    "\n",
    "    def display_info(self):\n",
    "        print(f\"Student ID: {self.student_id}\")\n",
    "        print(f\"Name: {self.name}\")\n",
    "        print(f\"Total Marks: {self.compute_total()}\")\n",
    "        print(f\"Average: {self.compute_average():.2f}\")\n",
    "        print(f\"Grade: {self.get_grade()}\")\n",
    "\n",
    "# Example usage\n",
    "student2 = Student(\"S022\", \"Francis Omara\", {\"Math\": 78, \"Science\": 82, \"English\": 75})\n",
    "student2.display_info()"
   ]
  },
