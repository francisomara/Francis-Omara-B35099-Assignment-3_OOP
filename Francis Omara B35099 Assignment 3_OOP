   "source": [
    "# Question 1.1. Create a parent class Vehicle with attributes: plate_number, owner, engine_cc, base_tax.\n",
    "\n",
    "class Vehicle:\n",
    "    def __init__(self, plate_number, owner, engine_cc, base_tax):\n",
    "        # Error handling for plate_number\n",
    "        if not isinstance(plate_number, str) or len(plate_number.strip()) == 0:\n",
    "            raise ValueError(\"Plate number must be a non-empty string.\")\n",
    "        self.plate_number = plate_number.strip().upper()\n",
    "\n",
    "        # Error handling for owner\n",
    "        if not isinstance(owner, str) or len(owner.strip()) == 0:\n",
    "            raise ValueError(\"Owner must be a non-empty string.\")\n",
    "        self.owner = owner.strip()\n",
    "\n",
    "        # Error handling for engine_cc\n",
    "        if not isinstance(engine_cc, (int, float)) or engine_cc <= 0:\n",
    "            raise ValueError(\"Engine CC must be a positive number.\")\n",
    "        self.engine_cc = engine_cc\n",
    "\n",
    "        # Error handling for base_tax\n",
    "        if not isinstance(base_tax, (int, float)) or base_tax < 0:\n",
    "            raise ValueError(\"Base tax must be a non-negative number.\")\n",
    "        self.base_tax = base_tax\n",
    "\n",
    "    def calculate_tax(self):\n",
    "        \"\"\"\n",
    "        Calculates the total tax based on engine capacity.\n",
    "        Uganda Vehicle tax can vary, e.g., for simplicity:\n",
    "        - Vehicles <= 1500cc: base_tax\n",
    "        - Vehicles > 1500cc: base_tax * 1.5\n",
    "        \"\"\"\n",
    "        if self.engine_cc <= 1500:\n",
    "            return self.base_tax\n",
    "        else:\n",
    "            return self.base_tax * 1.5\n",
    "\n",
    "    def display_info(self):\n",
    "        \"\"\"\n",
    "        Displays vehicle details.\n",
    "        \"\"\"\n",
    "        print(f\"Plate Number: {self.plate_number}\")\n",
    "        print(f\"Owner: {self.owner}\")\n",
    "        print(f\"Engine CC: {self.engine_cc}\")\n",
    "        print(f\"Base Tax: UGX {self.base_tax}\")\n",
    "        print(f\"Calculated Tax: UGX {self.calculate_tax()}\")\n",
    "\n",
    "\n",
    "try:\n",
    "    vehicle1 = Vehicle(\"UAA 123A\", \"Racheal Auma\", 1600, 500_000)\n",
    "    vehicle1.display_info()\n",
    "except ValueError as e:\n",
    "    print(f\"Error creating vehicle: {e}\")"
   ]
  },
