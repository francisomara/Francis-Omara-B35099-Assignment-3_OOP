   "source": [
    "# Question 1.4.Demonstrate polymorphism by calling calculate_tax() for all types.\n",
    "\n",
    "# --- Parent and Subclasses from Previous Steps ---\n",
    "\n",
    "class Vehicle:\n",
    "    def __init__(self, plate_number, owner, engine_cc, base_tax):\n",
    "        self.plate_number = plate_number\n",
    "        self.owner = owner\n",
    "        self.engine_cc = engine_cc\n",
    "        self.base_tax = base_tax\n",
    "\n",
    "    def calculate_tax(self):\n",
    "        \"\"\"Base tax (to be overridden by subclasses).\"\"\"\n",
    "        return self.base_tax\n",
    "\n",
    "\n",
    "class Car(Vehicle):\n",
    "    def __init__(self, plate_number, owner, engine_cc, base_tax, passenger_capacity):\n",
    "        super().__init__(plate_number, owner, engine_cc, base_tax)\n",
    "        self.passenger_capacity = passenger_capacity\n",
    "\n",
    "    def calculate_tax(self):\n",
    "        return self.base_tax + (self.engine_cc * 0.05)\n",
    "\n",
    "\n",
    "class Truck(Vehicle):\n",
    "    def __init__(self, plate_number, owner, engine_cc, base_tax, load_capacity):\n",
    "        super().__init__(plate_number, owner, engine_cc, base_tax)\n",
    "        self.load_capacity = load_capacity\n",
    "\n",
    "    def calculate_tax(self):\n",
    "        return self.base_tax + (self.load_capacity * 0.1)\n",
    "\n",
    "\n",
    "class Motorbike(Vehicle):\n",
    "    def __init__(self, plate_number, owner, engine_cc, base_tax, bike_type):\n",
    "        super().__init__(plate_number, owner, engine_cc, base_tax)\n",
    "        self.bike_type = bike_type\n",
    "\n",
    "    def calculate_tax(self):\n",
    "        return self.base_tax + 20_000\n",
    "\n",
    "\n",
    "# --- Demonstration of Polymorphism ---\n",
    "\n",
    "# Create objects of each type\n",
    "car = Car(\"UAA 123A\", \"Racheal\", 1800, 600_000, 5)\n",
    "truck = Truck(\"UBB 456B\", \"Cyrus\", 3000, 1_000_000, 2500)\n",
    "bike = Motorbike(\"UCC 789C\", \"Cephas\", 200, 200_000, \"boda\")\n",
    "\n",
    "# Store all vehicles in a list (parent type reference)\n",
    "vehicles = [car, truck, bike]\n",
    "\n",
    "# Loop through and call the same method name 'calculate_tax'\n",
    "print(\"=== Demonstrating Polymorphism ===\")\n",
    "for v in vehicles:\n",
    "    print(f\"\\nVehicle Type: {v.__class__.__name__}\")\n",
    "    print(f\"Owner: {v.owner}\")\n",
    "    print(f\"Plate Number: {v.plate_number}\")\n",
    "    print(f\"Calculated Tax: UGX {v.calculate_tax():,.0f}\")"
   ]
