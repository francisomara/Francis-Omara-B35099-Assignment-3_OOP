    "# 6.3.Calculate monthly averages using numpy.\n",
    "import numpy as np\n",
    "\n",
    "def monthly_average(df):\n",
    "    avg_buying = np.mean(df['buying_rate'])\n",
    "    avg_selling = np.mean(df['selling_rate'])\n",
    "    return avg_buying, avg_selling\n",
    "\n",
    "avg_buy, avg_sell = monthly_average(df)\n",
    "print(f\"Average buying rate: {avg_buy:.2f}, Average selling rate: {avg_sell:.2f}\")"
   ]
  },
