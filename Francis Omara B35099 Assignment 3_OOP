    "# 5.5.Add error handling for invalid tokens and API limits.\n",
    "\n",
    "import random\n",
    "\n",
    "class UgandaMusicAnalyticsSafe(UgandaMusicAnalytics):\n",
    "    def fetch_top_tracks_safe(self, token=\"valid_token\"):\n",
    "        try:\n",
    "            if token != \"valid_token\":\n",
    "                raise PermissionError(\"âŒ Invalid API token provided.\")\n",
    "            \n",
    "            if random.choice([True, False]):  # simulate rate limit randomly\n",
    "                raise ConnectionError(\"âš ï¸ API rate limit exceeded. Try again later.\")\n",
    "            \n",
    "            return self.mock_data\n",
    "        \n",
    "        except PermissionError as e:\n",
    "            print(e)\n",
    "            return {}\n",
    "        except ConnectionError as e:\n",
    "            print(e)\n",
    "            return {}\n",
    "        except Exception as e:\n",
    "            print(\"Unexpected error:\", e)\n",
    "            return {}\n",
    "\n",
    "safe_music = UgandaMusicAnalyticsSafe()\n",
    "safe_music.fetch_top_tracks_safe(\"invalid_token\")   # test invalid token\n",
    "safe_music.fetch_top_tracks_safe(\"valid_token\")     # test rate limit simulation"
   ]
