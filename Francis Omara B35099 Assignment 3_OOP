   "source": [
    "# Q6. The Bank of Uganda provides JSON-based daily exchange rate data (try to locate this online) for USD, GBP, and EUR. Attempt the following tasks: 6.1. Write a class ExchangeRatePipeline that fetches or loads data from local exchange_rates.json.\n",
    "\n",
    "import json\n",
    "import requests\n",
    "\n",
    "# Step 0: Save example JSON locally\n",
    "exchange_data = {\n",
    "    \"date\": \"2025-10-30\",\n",
    "    \"rates\": [\n",
    "        {\"currency_pair\": \"USD/UGX\", \"buying_rate\": 3463.14, \"selling_rate\": 3473.14},\n",
    "        {\"currency_pair\": \"GBP/USD\", \"buying_rate\": 4575.673725, \"selling_rate\": 4588.886225},\n",
    "        {\"currency_pair\": \"EUR/USD\", \"buying_rate\": 4025.727093, \"selling_rate\": 4037.351593}\n",
    "    ]\n",
    "}\n",
    "\n",
    "with open(\"exchange_rates.json\", \"w\") as f:\n",
    "    json.dump(exchange_data, f, indent=2)\n",
    "print(\"Saved exchange_rates.json locally.\")\n",
    "\n",
    "# Step 1: Define pipeline class\n",
    "class ExchangeRatePipeline:\n",
    "    def __init__(self, local_file=None, url=None):\n",
    "        self.local_file = local_file\n",
    "        self.url = url\n",
    "        self.data = None\n",
    "\n",
    "    def load_local(self):\n",
    "        try:\n",
    "            with open(self.local_file, \"r\") as f:\n",
    "                self.data = json.load(f)\n",
    "            print(f\"Loaded data from {self.local_file}\")\n",
    "            return self.data\n",
    "        except FileNotFoundError:\n",
    "            print(\"Local file not found.\")\n",
    "        except json.JSONDecodeError:\n",
    "            print(\"Error decoding JSON.\")\n",
    "\n",
    "    def fetch_online(self):\n",
    "        try:\n",
    "            response = requests.get(self.url)\n",
    "            response.raise_for_status()\n",
    "            self.data = response.json()\n",
    "            print(\"Fetched data from online source.\")\n",
    "            return self.data\n",
    "        except requests.HTTPError as e:\n",
    "            print(f\"HTTP Error: {e}\")\n",
    "        except requests.RequestException as e:\n",
    "            print(f\"Request failed: {e}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "id": "8769e5a7-5a04-47bf-801e-79c7ecffa3a9",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Loaded data from exchange_rates.json\n",
      "  currency_pair  buying_rate  selling_rate\n",
      "0       USD/UGX  3463.140000   3473.140000\n",
      "1       GBP/USD  4575.673725   4588.886225\n",
      "2       EUR/USD  4025.727093   4037.351593\n"
     ]
    }
   ],
   "source": [
    "# 6.2.Clean and transform data using pandas.\n",
    "import pandas as pd\n",
    "\n",
    "def clean_transform(data):\n",
    "    df = pd.json_normalize(data['rates'])\n",
    "    df = df[df['currency_pair'].isin(['USD/UGX', 'GBP/USD', 'EUR/USD'])]\n",
    "    df['buying_rate'] = pd.to_numeric(df['buying_rate'], errors='coerce')\n",
    "    df['selling_rate'] = pd.to_numeric(df['selling_rate'], errors='coerce')\n",
    "    return df\n",
    "\n",
    "pipeline = ExchangeRatePipeline(local_file=\"exchange_rates.json\")\n",
    "data = pipeline.load_local()\n",
    "df = clean_transform(data)\n",
    "print(df)"
   ]
  },
