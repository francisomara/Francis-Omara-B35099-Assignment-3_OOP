    "# 6.4.Plot exchange rate trends using Matplotlib.\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def plot_trends(df, date):\n",
    "    plt.figure(figsize=(8,5))\n",
    "    plt.plot(df['currency_pair'], df['buying_rate'], marker='o', label='Buying Rate')\n",
    "    plt.plot(df['currency_pair'], df['selling_rate'], marker='o', label='Selling Rate')\n",
    "    plt.title(f\"Exchange Rates on {date}\")\n",
    "    plt.xlabel(\"Currency Pair\")\n",
    "    plt.ylabel(\"Rate\")\n",
    "    plt.legend()\n",
    "    plt.grid(True)\n",
    "    plt.show()\n",
