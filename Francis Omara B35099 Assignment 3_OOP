    "# Question 1.3. Implement a method calculate_tax(): - Cars: base_tax + (engine_cc Ã— 0.05) - Trucks: base_tax + (load_capacity Ã— 0.1) - Motorbikes: base_tax + 20,000\n",
    "# Parent class\n",
    "class Vehicle:\n",
    "    def __init__(self, plate_number, owner, engine_cc, base_tax):\n",
    "        if not isinstance(plate_number, str) or len(plate_number.strip()) == 0:\n",
    "            raise ValueError(\"Plate number must be a non-empty string.\")\n",
    "        self.plate_number = plate_number.strip().upper()\n",
    "\n",
    "        if not isinstance(owner, str) or len(owner.strip()) == 0:\n",
    "            raise ValueError(\"Owner must be a non-empty string.\")\n",
    "        self.owner = owner.strip()\n",
    "\n",
    "        if not isinstance(engine_cc, (int, float)) or engine_cc <= 0:\n",
    "            raise ValueError(\"Engine CC must be a positive number.\")\n",
    "        self.engine_cc = engine_cc\n",
    "\n",
    "        if not isinstance(base_tax, (int, float)) or base_tax < 0:\n",
    "            raise ValueError(\"Base tax must be a non-negative number.\")\n",
    "        self.base_tax = base_tax\n",
    "\n",
    "    def calculate_tax(self):\n",
    "        \"\"\"Base class tax (generic). To be overridden by subclasses.\"\"\"\n",
    "        return self.base_tax\n",
    "\n",
    "    def display_info(self):\n",
    "        \"\"\"Display vehicle details.\"\"\"\n",
    "        print(f\"Plate Number: {self.plate_number}\")\n",
    "        print(f\"Owner: {self.owner}\")\n",
    "        print(f\"Engine CC: {self.engine_cc}\")\n",
    "        print(f\"Base Tax: UGX {self.base_tax}\")\n",
    "        print(f\"Calculated Tax: UGX {self.calculate_tax()}\")\n",
    "\n",
    "\n",
    "# Subclass for Car\n",
    "class Car(Vehicle):\n",
    "    def __init__(self, plate_number, owner, engine_cc, base_tax, passenger_capacity):\n",
    "        super().__init__(plate_number, owner, engine_cc, base_tax)\n",
    "        if not isinstance(passenger_capacity, int) or passenger_capacity <= 0:\n",
    "            raise ValueError(\"Passenger capacity must be a positive integer.\")\n",
    "        self.passenger_capacity = passenger_capacity\n",
    "\n",
    "    def calculate_tax(self):\n",
    "        \"\"\"Cars: base_tax + (engine_cc Ã— 0.05)\"\"\"\n",
    "        return self.base_tax + (self.engine_cc * 0.05)\n",
    "\n",
    "    def display_info(self):\n",
    "        super().display_info()\n",
    "        print(f\"Passenger Capacity: {self.passenger_capacity}\")\n",
    "\n",
    "\n",
    "# Subclass for Truck\n",
    "class Truck(Vehicle):\n",
    "    def __init__(self, plate_number, owner, engine_cc, base_tax, load_capacity):\n",
    "        super().__init__(plate_number, owner, engine_cc, base_tax)\n",
    "        if not isinstance(load_capacity, (int, float)) or load_capacity <= 0:\n",
    "            raise ValueError(\"Load capacity must be a positive number.\")\n",
    "        self.load_capacity = load_capacity\n",
    "\n",
    "    def calculate_tax(self):\n",
    "        \"\"\"Trucks: base_tax + (load_capacity Ã— 0.1)\"\"\"\n",
    "        return self.base_tax + (self.load_capacity * 0.1)\n",
    "\n",
    "    def display_info(self):\n",
    "        super().display_info()\n",
    "        print(f\"Load Capacity: {self.load_capacity} kg\")\n",
    "\n",
    "\n",
    "# Subclass for Motorbike\n",
    "class Motorbike(Vehicle):\n",
    "    def __init__(self, plate_number, owner, engine_cc, base_tax, bike_type):\n",
    "        super().__init__(plate_number, owner, engine_cc, base_tax)\n",
    "        if not isinstance(bike_type, str) or len(bike_type.strip()) == 0:\n",
    "            raise ValueError(\"Motorbike type must be a non-empty string.\")\n",
    "        self.bike_type = bike_type.strip().lower()\n",
    "\n",
    "    def calculate_tax(self):\n",
    "        \"\"\"Motorbikes: base_tax + 20,000\"\"\"\n",
    "        return self.base_tax + 20_000\n",
    "\n",
    "    def display_info(self):\n",
    "        super().display_info()\n",
    "        print(f\"Motorbike Type: {self.bike_type}\")\n",
