   ],
   "source": [
    "# 4.3. Subclass PostgraduateStudent adds research_topic and evaluate_thesis() returning 'Pass' or 'Revise'.\n",
    "\n",
    "class PostgraduateStudent(Student):\n",
    "    def __init__(self, student_id, name, marks, research_topic, thesis_score):\n",
    "        super().__init__(student_id, name, marks)\n",
    "        self.research_topic = research_topic\n",
    "        self.thesis_score = thesis_score\n",
    "\n",
    "    def evaluate_thesis(self):\n",
    "        return \"Pass\" if self.thesis_score >= 70 else \"Revise\"\n",
    "\n",
    "    def display_info(self):\n",
    "        super().display_info()\n",
