    "# 5.2.Use pandas to create a DataFrame of track names, play counts, and popularity.\n",
    "\n",
    "import pandas as pd\n",
    "\n",
    "class UgandaMusicAnalytics(UgandaMusicAnalytics):\n",
    "    def create_dataframe(self):\n",
    "        records = []\n",
    "        for artist in self.mock_data[\"artists\"]:\n",
    "            for track in artist[\"tracks\"]:\n",
    "                records.append({\n",
    "                    \"artist\": artist[\"name\"],\n",
    "                    \"track\": track[\"title\"],\n",
    "                    \"play_count\": track[\"play_count\"],\n",
    "                    \"popularity\": track[\"popularity\"],\n",
    "                    \"produced_in\": track[\"produced_in\"]\n",
    "                })\n",
    "        df = pd.DataFrame(records)\n",
    "        print(\"âœ… DataFrame created successfully!\")\n",
    "        return df\n",
    "\n",
    "# Example usage\n",
    "music = UgandaMusicAnalytics()\n",
    "df = music.create_dataframe()\n",
