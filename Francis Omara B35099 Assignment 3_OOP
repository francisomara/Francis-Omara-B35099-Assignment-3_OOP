   "source": [
    "# 2.5 Display equivalent loan eligibility in UGX and USD\n",
    "\n",
    "def display_loan_in_usd(member):\n",
    "    ugx_amount = member.loan_eligibility()\n",
    "    rate = Member.get_exchange_rate()\n",
    "    if rate is None:\n",
    "        print(f\"{member.name}'s loan eligibility: {ugx_amount} UGX (USD rate unavailable)\")\n",
    "        return\n",
    "    usd_amount = ugx_amount / rate\n",
    "    print(f\"{member.name}'s loan eligibility: {ugx_amount} UGX â‰ˆ {usd_amount:.2f} USD\")\n",
    "\n",
    "# Example usage with previously defined subclasses:\n",
    "class FarmerMember(Member):\n",
    "    def loan_eligibility(self):\n",
    "        return self.savings_balance * 6\n",
    "\n",
    "class TraderMember(Member):\n",
    "    def loan_eligibility(self):\n",
    "        return self.savings_balance * 4\n",
    "\n",
    "farmer = FarmerMember(\"Esther\", \"F001\", 5000)\n",
    "trader = TraderMember(\"Byron\", \"T001\", 8000)\n",
    "\n",
    "display_loan_in_usd(farmer)\n",
    "display_loan_in_usd(trader)"
   ]
  },
