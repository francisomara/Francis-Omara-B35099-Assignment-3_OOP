    "# Question 2.2.\n",
    "from abc import ABC, abstractmethod\n",
    "\n",
    "# Abstract base class\n",
    "class Member(ABC):\n",
    "    def __init__(self, name, id_no, savings_balance):\n",
    "        self.name = name\n",
    "        self.id_no = id_no\n",
    "        self.savings_balance = savings_balance\n",
    "\n",
    "    @abstractmethod\n",
    "    def loan_eligibility(self):\n",
    "        pass\n",
    "\n",
    "# Farmer subclass\n",
    "class FarmerMember(Member):\n",
    "    def loan_eligibility(self):\n",
    "        return 6 * self.savings_balance\n",
    "\n",
    "# Trader subclass\n",
    "class TraderMember(Member):\n",
    "    def loan_eligibility(self):\n",
    "        return 4 * self.savings_balance\n",
    "\n",
    "# Example usage\n",
    "farmer = FarmerMember(\"Esther\", \"F001\", 5000)\n",
    "trader = TraderMember(\"Byron\", \"T001\", 8000)\n",
    "\n",
    "print(f\"{farmer.name}'s loan eligibility: {farmer.loan_eligibility()} UGX\")\n",
    "print(f\"{trader.name}'s loan eligibility: {trader.loan_eligibility()} UGX\")\n"
   ]
