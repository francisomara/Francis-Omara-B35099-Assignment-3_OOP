    "# Q4. Build a student grading system. Requirements: 4.1. Base class Student with: student_id, name, marks (dictionary of subjects).\n",
    "\n",
    "class Student:\n",
    "    def __init__(self, student_id, name, marks):\n",
    "        self.student_id = student_id\n",
    "        self.name = name\n",
    "        self.marks = marks  # dictionary e.g., {\"Math\": 85, \"Science\": 78}\n",
    "    \n",
    "    def display_info(self):\n",
    "        print(f\"Student ID: {self.student_id}\")\n",
    "        print(f\"Name: {self.name}\")\n",
    "        print(\"Marks:\")\n",
    "        for subject, mark in self.marks.items():\n",
    "            print(f\"  {subject}: {mark}\")\n",
