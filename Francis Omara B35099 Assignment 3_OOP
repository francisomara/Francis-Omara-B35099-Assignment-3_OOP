    "#2.6 Include exception handling for networkJSON errors.\n",
    "\n",
    "# but we can wrap display function as well\n",
    "\n",
    "def display_loan_with_error_handling(member):\n",
    "    try:\n",
    "        ugx_amount = member.loan_eligibility()\n",
    "        rate = Member.get_exchange_rate()\n",
    "        if rate is None or rate <= 0:\n",
    "            raise ValueError(\"Invalid USD/UGX rate\")\n",
    "        usd_amount = ugx_amount / rate\n",
    "        print(f\"{member.name}'s loan eligibility: {ugx_amount} UGX â‰ˆ {usd_amount:.2f} USD\")\n",
    "    except ValueError as ve:\n",
    "        print(f\"{member.name}'s loan eligibility: {ugx_amount} UGX ({ve})\")\n",
    "    except Exception as e:\n",
    "        print(f\"{member.name}'s loan eligibility: {ugx_amount} UGX (Error: {e})\")\n",
    "\n",
    "# Example usage:\n",
    "display_loan_with_error_handling(farmer)\n",
    "display_loan_with_error_handling(trader)"
   ]
