    "from abc import ABC, abstractmethod\n",
    "\n",
    "class Member(ABC):\n",
    "    def __init__(self, name, id_no, savings_balance):\n",
    "        self.name = name\n",
    "        self.id_no = id_no\n",
    "        self.savings_balance = savings_balance\n",
    "\n",
    "    @abstractmethod\n",
    "    def loan_eligibility(self):\n",
