   "source": [
    "# 3.2.Subclass SmartTrafficLight that: Connects to a simulated sensor API returning car density. Adjusts green/red duration based on car count.\n",
    "\n",
    "import random\n",
    "import json\n",
    "\n",
    "# --- Simulated Sensor API ---\n",
    "def simulated_sensor_api(location):\n",
    "    \"\"\"\n",
    "    Simulate a sensor API returning car density in JSON format.\n",
    "    \"\"\"\n",
    "    sensor_data = {\n",
    "        \"location\": location,\n",
    "        \"car_count\": random.randint(0, 50)  # Random car density\n",
    "    }\n",
    "    # Convert Python dict to JSON string (like a real API response)\n",
    "    return json.dumps(sensor_data)\n",
    "\n",
    "\n",
    "# --- Display JSON Data Samples ---\n",
    "print(\"Sample JSON data from simulated API:\")\n",
    "for _ in range(3):\n",
    "    json_sample = simulated_sensor_api(\"Jinja Road\")\n",
    "    print(json_sample)\n",
    "\n",
    "print(\"\\nFormatted JSON samples:\")\n",
    "for _ in range(3):\n",
    "    json_sample = simulated_sensor_api(\"Jinja Road\")\n",
    "    parsed = json.loads(json_sample)\n",
    "    print(json.dumps(parsed, indent=4))\n",
    "\n",
    "\n",
    "# --- Base Class: TrafficLight ---\n",
    "class TrafficLight:\n",
    "    def __init__(self, location):\n",
    "        self.location = location\n",
    "        self._current_state = \"Red\"  # Private attribute for encapsulation\n",
    "\n",
    "    def turn_green(self):\n",
    "        self._current_state = \"Green\"\n",
    "\n",
    "    def turn_red(self):\n",
    "        self._current_state = \"Red\"\n",
    "\n",
    "    def status(self):\n",
    "        return f\"{self.location} traffic light is {self._current_state}\"\n",
    "\n",
    "\n",
    "# --- Subclass: SmartTrafficLight ---\n",
    "class SmartTrafficLight(TrafficLight):\n",
    "    def __init__(self, location):\n",
    "        super().__init__(location)\n",
    "        self.green_duration = 30  # default values\n",
    "        self.red_duration = 30\n",
    "\n",
    "    def get_sensor_data(self):\n",
    "        \"\"\"Connect to the simulated API and parse JSON.\"\"\"\n",
    "        response = simulated_sensor_api(self.location)\n",
    "        data = json.loads(response)  # Convert JSON string to Python dict\n",
    "        return data\n",
    "\n",
    "    def adjust_durations(self):\n",
    "        \"\"\"Adjust light durations based on car density.\"\"\"\n",
    "        data = self.get_sensor_data()\n",
    "        car_count = data[\"car_count\"]\n",
    "\n",
    "        if car_count > 30:\n",
    "            self.green_duration = 60\n",
    "            self.red_duration = 30\n",
    "        elif car_count > 15:\n",
    "            self.green_duration = 45\n",
    "            self.red_duration = 45\n",
    "        else:\n",
    "            self.green_duration = 30\n",
    "            self.red_duration = 60\n",
    "\n",
    "        return car_count\n",
    "\n",
    "    def cycle(self):\n",
    "        \"\"\"Simulate one traffic light cycle.\"\"\"\n",
    "        car_count = self.adjust_durations()\n",
    "        self.turn_green()\n",
    "        print(f\"\\n{self.location}: Green for {self.green_duration}s, Red for {self.red_duration}s (Cars detected: {car_count})\")\n",
    "        self.turn_red()\n",
    "        print(f\"Status after cycle: {self.status()}\")\n"
   ]
  },
