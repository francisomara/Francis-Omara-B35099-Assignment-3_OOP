   "source": [
    "# 3.4. Simulate multiple cycles printing logs like: [09:15] Jinja Road: Green for 60s, Red for 30s.\n",
    "\n",
    "from datetime import datetime, timedelta\n",
    "\n",
    "def simulate_cycles(light, cycles=5, start_time=\"09:15\"):\n",
    "    time_obj = datetime.strptime(start_time, \"%H:%M\")\n",
    "    for i in range(cycles):\n",
    "        car_count = light.adjust_durations()\n",
    "        light.turn_green()\n",
    "        print(f\"[{time_obj.strftime('%H:%M')}] {light.location}: Green for {light.green_duration}s, Red for {light.red_duration}s (Cars: {car_count})\")\n",
    "        light.turn_red()\n",
    "        # Increment time for simulation\n",
    "        time_obj += timedelta(minutes=1)\n",
    "\n",
    "# Run simulation\n",
    "smart_light = SmartTrafficLight(\"Jinja Road\")\n",
    "simulate_cycles(smart_light, cycles=5)"
   ]
  },
