   "execution_count": 41,
   "id": "ac176e74-74d8-4562-a916-dd402dd34718",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 6.6.Include exception handling for missing data or HTTP errors\n",
    "import json\n",
    "import requests\n",
    "\n",
    "class ExchangeRatePipeline:\n",
    "    def __init__(self, local_file=None, url=None):\n",
    "        self.local_file = local_file\n",
    "        self.url = url\n",
    "        self.data = None\n",
    "\n",
    "    def load_local(self):\n",
    "        if not self.local_file:\n",
    "            print(\"Error: No local file path provided.\")\n",
    "            return None\n",
    "        try:\n",
    "            with open(self.local_file, \"r\") as f:\n",
    "                self.data = json.load(f)\n",
    "            print(f\"Loaded data from {self.local_file}\")\n",
    "            return self.data\n",
    "        except FileNotFoundError:\n",
    "            print(f\"Error: Local file '{self.local_file}' not found.\")\n",
    "        except json.JSONDecodeError:\n",
    "            print(f\"Error: Failed to decode JSON from '{self.local_file}'.\")\n",
    "        except Exception as e:\n",
    "            print(f\"Unexpected error while loading local file: {e}\")\n",
    "\n",
    "    def fetch_online(self):\n",
    "        if not self.url:\n",
    "            print(\"Error: No URL provided.\")\n",
    "            return None\n",
    "        try:\n",
    "            response = requests.get(self.url, timeout=10)\n",
    "            response.raise_for_status()\n",
    "            self.data = response.json()\n",
    "            print(\"Fetched data from online source successfully.\")\n",
    "            return self.data\n",
    "        except requests.HTTPError as e:\n",
    "            print(f\"HTTP Error: {e}\")\n",
    "        except requests.ConnectionError:\n",
    "            print(\"Error: Failed to connect to the URL.\")\n",
    "        except requests.Timeout:\n",
    "            print(\"Error: Request timed out.\")\n",
    "        except requests.RequestException as e:\n",
    "            print(f\"Request failed: {e}\")\n",
    "        except Exception as e:\n",
    "            print(f\"Unexpected error during fetch: {e}\")\n"
   ]
  },
